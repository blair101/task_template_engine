{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "required": [
        "key",
        "document",
        "labels"
    ],
    "properties": {
        "key": {
            "type": "string"
        },
        "document": {
            "type": "object"
        },
        "labels": {
            "patternProperties": {
                "^\\w+$": {
                    "type":"string",
                    "pattern": "^\\w+$"
                }
            }
        }
    }
}